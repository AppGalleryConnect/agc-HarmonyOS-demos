    "sceneryAudioUrl": "",
    "sceneryId": "",
    "sceneryPlayImg": "/common/play.png",
    "ticketName": ""
  },
  "actions": {
    "agendaDetail": {
      "action": "router",
      "abilityName": "com.smarket.hdc2023.MainAbility",
      "params": {
        "message": "agendaDetail",
        "data": "{{agendaSwiperData[agendaSwiperIndex].subSeminarId}}",
        "data2": "{{agendaSwiperData[agendaSwiperIndex].hasGroupName}}",
        "data3": "{{agendaSwiperData[agendaSwiperIndex].startTime}}"
      }
    },
    "myOrAllAgenda": {
      "action": "router",
      "abilityName": "com.smarket.hdc2023.MainAbility",
      "params": {
        "message": "myOrAllAgenda",
        "data": "{{style.notMyAgendaCardBtn}}"
      }
    },
    "agendaSwiperDataChange": {
      "action": "message",
      "params": {
        "action": "agendaSwiperDataChange",
        "message": "{{agendaSwiperIndex}}"
      }
    },
    "myTickets": {
      "action": "router",
      "abilityName": "com.smarket.hdc2023.MainAbility",
      "params": {
        "message": "myTickets"
      }
    },
    "goIntegral": {
      "action": "router",
      "abilityName": "com.smarket.hdc2023.MainAbility",
      "params": {
        "message": "goIntegral"
      }
    },
    "goPointsShop": {
      "action": "router",
      "abilityName": "com.smarket.hdc2023.widget.form.TransparentAbility",
      "params": {
        "message": "goPointsShop"
      }
    },
    "goTask": {
      "action": "router",
      "abilityName": "com.smarket.hdc2023.widget.form.TransparentAbility",
      "params": {
        "message": "goTask",
        "data": "{{taskCardText}}",
        "data2": "{{cardId}}",
        "data3": "{{distanceMessage}}",
        "data4": "{{interactionTaskId}}"
      }
    },
    "goPlay": {
      "action": "message",
      "abilityName": "com.smarket.hdc2023.MainAbility",
      "params": {
        "action": "goPlay",
        "sceneryAudioUrl": "{{sceneryAudioUrl}}",
        "sceneryCardId": "{{cardId}}",
        "sceneryMessage": "{{distanceMessage}}",
        "sceneryTaskId": "{{interactionTaskId}}"
      }
    },
    "sceneryDetail": {
      "action": "router",
      "abilityName": "com.smarket.hdc2023.MainAbility",
      "params": {
        "message": "sceneryDetail",
        "data": "{{sceneryId}}"
      }
    },
    "goLogin": {
      "action": "router",
      "abilityName": "com.smarket.hdc2023.MainAbility",
      "params": {
        "message": "goLogin"
      }
    }
  }
}